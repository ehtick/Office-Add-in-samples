{
  "$schema": "https://developer.microsoft.com/json-schemas/teams/v1.25/MicrosoftTeams.schema.json",
  "manifestVersion": "1.25",
  "version": "1.0.0",
  "id": "d163188c-15eb-491f-9e12-61c0b72c904c",
  "localizationInfo": {
    "defaultLanguageTag": "en-us"
  },
  "developer": {
    "name": "Contoso",
    "websiteUrl": "https://www.contoso.com",
    "privacyUrl": "https://www.contoso.com/privacy",
    "termsOfUseUrl": "https://www.contoso.com/terms"
  },
  "name": {
    "short": "Excel DV Content Add-in",
    "full": "Excel Data Visualization Content Add-in"
  },
  "description": {
    "short": "A content add-in for Excel showing data visualization.",
    "full": "A content add-in for Excel showing the data visualization option."
  },
  "icons": {
    "outline": "assets/icon-32.png",
    "color": "assets/icon-64.png"
  },
  "accentColor": "#FFFFFF",
  "validDomains": [
    "AppDomain1",
    "AppDomain2",
    "AppDomain3"
  ],
  "authorization": {
    "permissions": {
      "resourceSpecific": [
        {
          "name": "Document.ReadWrite.User",
          "type": "Delegated"
        }
      ]
    }
  },
  "extensions": [
    {
      "requirements": {
        "scopes": [ "workbook" ],
        "capabilities": [
          {
            "name": "ExcelApi",
            "minVersion": "1.1"
          },
          {
            "name": "TableBindings",
            "minVersion": "1.1"
          }
        ]
      },
      "contentRuntimes": [
        {
          "id": "ContentRuntime",
          "code": {
            "page": "https://officedev.github.io/Office-Add-in-samples/Samples/excel-content-data-visualization/dist/home.html"
          },
          "requestedWidth": 400,
          "requestedHeight": 400,
          "disableSnapshot": true
        }
      ]
    }
  ]
}
